<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jasmeet.track_n_trace" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddActivity"
            android:label="@string/title_activity_add" >
        </activity>
        <activity
            android:name=".AddRegActivity"
            android:label="@string/title_activity_add_reg" >
        </activity>
        <activity
            android:name=".AddGatewaysActivity"
            android:label="@string/title_activity_add_gateways" >
        </activity>
        <activity
            android:name=".AddAssetsActivity"
            android:label="@string/title_activity_add_assets" >
        </activity>
    </application>

</manifest>
